.account-page
  .account-page__content
    .account-page__btn#form
      %h2 賛同署名フォーム
      = render "devise/shared/links"

    .account-page__btn#member
      %h2  Members Station
      %h5  Log in
      .account-page__user-form
        = form_for(resource, as: resource_name, url: session_path(resource_name)) do |f|
          .field
            .field-label
              = f.label :email
            .field-input
              = f.email_field :email, autofocus: true
            .field
              .field-label
                = f.label :password
                %i (英数字8文字以上)
              .field-input
                = f.password_field :password, autocomplete: "off"
            .actions
              = f.submit "Log in", class: 'btn'
